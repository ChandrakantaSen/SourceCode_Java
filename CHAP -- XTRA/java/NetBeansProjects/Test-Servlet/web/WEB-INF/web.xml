<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>demoServlet</servlet-name>
        <servlet-class>demoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>add</servlet-name>
        <servlet-class>add</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SumofDigit</servlet-name>
        <servlet-class>myPkg.SumofDigit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReverseofDigit</servlet-name>
        <servlet-class>myPkg.ReverseofDigit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SecndHighest</servlet-name>
        <servlet-class>myPkg.SecndHighest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HeaderInfoDisplay</servlet-name>
        <servlet-class>myPkg.HeaderInfoDisplay</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>demoServlet</servlet-name>
        <url-pattern>/demoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>add</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SumofDigit</servlet-name>
        <url-pattern>/SumofDigit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReverseofDigit</servlet-name>
        <url-pattern>/ReverseofDigit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SecndHighest</servlet-name>
        <url-pattern>/SecndHighest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HeaderInfoDisplay</servlet-name>
        <url-pattern>/HeaderInfoDisplay</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
