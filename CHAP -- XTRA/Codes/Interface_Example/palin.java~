import java.io.*;

class palin
{
  int n;
  palin()
  {
    n=0;
  }
  
  void getdata()
  {
    BufferedReader in = new BufferedReader(new InputStreamReader(System.in));
    System.out.println("Enter the number: ");
    n = Integer.parseInt(in.readLine());
  }
  
  void calc()
  {
    int p=n;
    int d,sum=0,
    
    while(p!=0)
    {
      d=p%10;
      sum = sum*10 +d;
      p=p/10;
    }
  }
}